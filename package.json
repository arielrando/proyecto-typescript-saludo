{
  "name": "backend-typescript",
  "version": "1.0.0",
  "description": "Proyecto en typecript",
  "main": "dist/index.js",
  "dependencies": {
    "dotenv": "16.0.0",
    "express": "4.18.0",
    "openmeteo": "1.2.2",
    "winston":"3.18.3",
    "cookie-parser":"1.4.7"
  },
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/express": "4.17.25",
    "@types/node": "20.0.0",
    "ts-node-dev": "2.0.0",
    "typescript": "^5.6.3",
    "@types/cookie-parser":"1.4.10"
  },
  "scripts": {
    "start": "node dist/index.js",
    "nodemon": "nodemon -L index.js",
    "dev": "ts-node-dev --respawn --transpile-only --poll src/index.ts",
    "build": "tsc"
  }
}
